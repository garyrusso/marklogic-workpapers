.container
  h1 {{workpaper.title}}
  .row
    .col-md-6
      h3(ng-show="workpaper.pending") Pending
      h3 Submitted-on {{workpaper.submitted | date}}

    .col-md-6
      .panel.panel-primary
        .panel-heading Tags
        .panel-body
          div(ng-repeat="tag in workpaper.tags") {{tag}}